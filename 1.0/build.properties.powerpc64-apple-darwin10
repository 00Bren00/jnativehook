#ant configure for apple powerpc64

project.debug=false
java.target=1.5
native.arch=powerpc64
native.os=osx
native.cc=/opt/powerpc64-apple-darwin10/bin/powerpc64-apple-darwin10-gcc
native.ld=/opt/powerpc64-apple-darwin10/bin/powerpc64-apple-darwin10-gcc
native.make=/usr/bin/make
native.strip=/opt/powerpc64-apple-darwin10/bin/powerpc64-apple-darwin10-strip
native.cflags=-c -m64 -arch ppc64 -mtune\=powerpc64 -mmacosx-version-min=10.4 -Wall -O2 -fomit-frame-pointer -fno-strict-aliasing -fPIC
native.ldflags=-dynamiclib -m64 -arch ppc64
native.libs=-framework ApplicationServices
native.includes=-I/opt/powerpc64-apple-darwin10/usr/include -I/opt/powerpc64-apple-darwin10/usr/lib/gcc/powerpc-apple-darwin10/4.0.1/include -I/opt/powerpc64-apple-darwin10/System/Library/Frameworks/JavaVM.framework/Versions/1.5.0/Headers
