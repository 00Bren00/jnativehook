#ant configure for windows x86_64
java.target=1.5
native.arch=x86-64
native.os=windows
native.cc=/root/crossdev/x86_64-w64-mingw32/cross_win64/bin/x86_64-w64-mingw32-gcc
native.ld=/root/crossdev/x86_64-w64-mingw32/cross_win64/bin/x86_64-w64-mingw32-ld
native.make=/usr/bin/make
native.cflags=-c -m64 -march\=x86-64 -mtune\=generic -std=c99 -pedantic -Wall -O2 -fno-strict-aliasing
native.ldflags=--as-needed -shared -static --kill-at -O2
native.libs=-L/root/crossdev/x86_64-w64-mingw32/cross_win64/mingw/lib/ -lcrtdll -luser32 -lkernel32
native.includes=-I/root/crossdev/x86_64-w64-mingw32/win32_jdk/include -I/root/crossdev/x86_64-w64-mingw32/win32_jdk/include/win32
project.debug=false
