#ant configure for apple powerpc

project.debug=false
java.target=1.5
native.arch=powerpc
native.os=osx
native.cc=/root/crossdev/powerpc-apple-darwin10/bin/powerpc-apple-darwin10-gcc
native.ld=/root/crossdev/powerpc-apple-darwin10/bin/powerpc-apple-darwin10-gcc
native.make=make
native.strip=/root/crossdev/powerpc-apple-darwin10/bin/powerpc-apple-darwin10-strip
native.cflags=-c -m32 -arch ppc -mtune\=powerpc -Wall -O2 -fomit-frame-pointer -fno-strict-aliasing
native.ldflags=-dynamiclib -m32 -arch ppc
native.libs=-framework Carbon -framework JavaVM
native.includes=-I/root/crossdev/powerpc-apple-darwin10/usr/include -I/root/crossdev/powerpc-apple-darwin10/usr/lib/gcc/powerpc-apple-darwin10/4.0.1/include -I/root/crossdev/powerpc-apple-darwin10/System/Library/Frameworks/JavaVM.framework/Versions/1.5.0/Headers
