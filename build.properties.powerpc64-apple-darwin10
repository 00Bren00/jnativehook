#ant configure for apple powerpc64

project.debug=false
java.target=1.5
native.arch=powerpc64
native.os=osx
native.cc=/root/crossdev/powerpc64-apple-darwin10/bin/powerpc64-apple-darwin10-gcc
native.ld=/root/crossdev/powerpc64-apple-darwin10/bin/powerpc64-apple-darwin10-gcc
native.make=make
native.strip=/root/crossdev/powerpc64-apple-darwin10/bin/powerpc64-apple-darwin10-strip
native.cflags=-c -m64 -arch ppc64 -mtune\=powerpc64 -Wall -O2 -fomit-frame-pointer -fno-strict-aliasing
native.ldflags=-dynamiclib -m64 -arch ppc64
native.libs=-framework Carbon -framework JavaVM
native.includes=-I/root/crossdev/powerpc64-apple-darwin10/usr/include -I/root/crossdev/powerpc64-apple-darwin10/usr/lib/gcc/powerpc-apple-darwin10/4.0.1/include -I/root/crossdev/powerpc64-apple-darwin10/System/Library/Frameworks/JavaVM.framework/Versions/1.5.0/Headers
