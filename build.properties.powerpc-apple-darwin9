#ant configure for apple powerpc

project.debug=false
java.target=1.5
native.arch=powerpc
native.os=osx
native.cc=/usr/bin/powerpc-apple-darwin9-gcc
native.ld=/usr/bin/powerpc-apple-darwin9-gcc
native.make=make
native.strip=/usr/bin/powerpc-apple-darwin9-strip
native.cflags=-c -m32 -arch ppc -mtune\=powerpc -Wall -O2 -fomit-frame-pointer -fno-strict-aliasing -fPIC
native.ldflags=-dynamiclib -m32 -arch ppc
native.libs=-framework Carbon -framework JavaVM
native.includes=-I/usr/lib/gcc/powerpc-apple-darwin9/4.0.1/include -I/usr/powerpc-apple-darwin9/usr/include -I/usr/powerpc-apple-darwin9/System/Library/Frameworks/JavaVM.framework/Versions/1.5.0/Headers
