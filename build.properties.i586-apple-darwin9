#ant configure for apple i586

project.debug=false
java.target=1.5
native.arch=i586
native.os=osx
native.cc=/root/crossdev/i586-apple-darwin9/bin/i586-apple-darwin9-gcc
native.ld=/root/crossdev/i586-apple-darwin9/bin/i586-apple-darwin9-ld
native.make=make
native.strip=/root/crossdev/i586-apple-darwin9/bin/i586-apple-darwin9-strip

native.cflags=-c -m32 -mtune\=i586 -arch i386 -Wall -O2 -fomit-frame-pointer -fno-strict-aliasing
native.ldflags=-arch i386 -dylib -dynamic 
native.libs=-lc -ldylib1.o -framework Carbon -framework JavaVM
native.includes=-I/root/crossdev/i586-apple-darwin9/usr/lib/gcc/i686-apple-darwin9/4.0.1/include -I/root/crossdev/i586-apple-darwin9/Developer/Headers -I/root/crossdev/i586-apple-darwin9/usr/include -I/root/crossdev/i586-apple-darwin9/System/Library/Frameworks/JavaVM.framework/Versions/1.5.0/Headers
